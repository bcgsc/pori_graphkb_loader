{
    "api": {
        "properties":{
            "protocolSection":{
                "properties":{
                    "identificationModule":{
                        "properties":{
                            "nctId":{
                                "items": {
                                    "pattern": "^NCT\\d+$"
                                },
                                "type": "string"
                            },
                            "briefTitle":{
                                "type": "string"
                            }
                        },
                        "required":[
                            "nctId",
                            "briefTitle"
                        ],
                        "type": "object"
                    },
                    "statusModule":{
                        "properties":{
                            "startDateStruct":{
                                "properties":{
                                    "date": {
                                        "type": "string"
                                    }
                                },
                                "required":[
                                    "date"
                                ],
                                "type": "object"
                            },
                            "completionDateStruct":{
                                "properties":{
                                    "date": {
                                        "type": "string"
                                    }
                                },
                                "required":[
                                    "date"
                                ],
                                "type": "object"
                            },
                            "lastUpdatePostDateStruct":{
                                "properties":{
                                    "date": {
                                        "type": "string"
                                    }
                                },
                                "required":[
                                    "date"
                                ],
                                "type": "object"
                            },
                            "overallStatus":{
                                "type": "string"
                            }
                        },
                        "required":[
                            "startDateStruct",
                            "completionDateStruct",
                            "lastUpdatePostDateStruct",
                            "overallStatus"
                        ],
                        "type": "object"
                    },
                    "conditionsModule":{
                        "properties":{
                            "conditions":{
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            }
                        },
                        "required":[
                            "conditions"
                        ],
                        "type": "object"
                    },
                    "designModule":{
                        "properties":{
                            "phases":{
                                "items": {
                                    "type": "string"
                                },
                                "type": "array"
                            }
                        },
                        "required":[
                            "phases"
                        ],
                        "type": "object"
                    },
                    "armsInterventionsModule":{
                        "properties":{
                            "interventions":{
                                "properties":{
                                    "type":{"type": "string"},
                                    "name":{"type": "string"}
                                },
                                "required":[
                                    "type",
                                    "name"
                                ],
                                "type": "array"
                            }
                        },
                        "required":[
                            "interventions"
                        ],
                        "type": "object"
                    }
                },
                "optionalProperties":{
                    "contactsLocationsModule":{
                        "optionalProperties":{
                            "locations":{
                                "properties":{
                                    "city":{"type": "string"},
                                    "country":{"type": "string"}
                                },
                                "required":[
                                    "city",
                                    "country"
                                ],
                                "type":"array"
                            }
                        },
                        "type": "object"
                    }
                },
                "required":[
                    "identificationModule",
                    "statusModule",
                    "conditionsModule",
                    "designModule",
                    "armsInterventionsModule"
                ],
                "type": "object"
            }
        },
        "required":[
            "protocolSection"
        ],
        "type": "object"
    },
    "rss": {
        "properties": {
            "rss": {
                "properties": {
                    "channel": {
                        "items": {
                            "properties": {
                                "item": {
                                    "items": {
                                        "properties": {
                                            "guid": {
                                                "items": {
                                                    "properties": {
                                                        "_": {
                                                            "pattern": "^NCT\\d+$",
                                                            "type": "string"
                                                        }
                                                    },
                                                    "required": [
                                                        "_"
                                                    ],
                                                    "type": "object"
                                                },
                                                "maxItems": 1,
                                                "minItems": 1,
                                                "type": "array"
                                            }
                                        },
                                        "required": [
                                            "guid"
                                        ],
                                        "type": "object"
                                    },
                                    "type": "array"
                                }
                            },
                            "required": [
                                "item"
                            ],
                            "type": "object"
                        },
                        "maxItems": 1,
                        "minItems": 1,
                        "type": "array"
                    }
                },
                "required": [
                    "channel"
                ],
                "type": "object"
            }
        },
        "required": [
            "rss"
        ],
        "type": "object"
    }
}
